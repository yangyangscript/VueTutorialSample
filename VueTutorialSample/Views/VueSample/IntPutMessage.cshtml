@section head
{
    <style>
        body {
            margin: 50px;
            padding: 0px;
        }
    </style>
}

@*
    1.v-model <input>、<select>、<textarea>、components中使用
*@

<div id="app">
    <p>{{message}}</p>
    <input v-model="message" />
</div>

@section script
{
    <script>
        var app = new Vue({
            el: '#app',
            data: {
                message: ''
            }
        })
    </script>
}
